; ;section 1.1.4, page 12
; (newline)

(define (square x) (* x x))

; ; section 1.1.4, page 13

(print (square 21))
(print (square (+ 2 5)))
(print (square (square 3)))
(define (sum-of-squares x y)
  (+ (square x) (square y)))
(print (sum-of-squares 3 4))
(define (f a)
  (sum-of-squares (+ a 1) (* a 2)))
(print (f 5))
